<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg4"

    tools:context=".MainActivity3">


    <androidx.cardview.widget.CardView
        android:layout_width="407dp"
        android:layout_height="527dp"

        app:cardCornerRadius="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">


        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewofinventory"
            android:layout_width="330dp"
            android:layout_height="120dp"
            android:layout_marginLeft="49dp"
            android:layout_marginTop="100dp"
            app:cardBackgroundColor="#E4A002"
            app:cardCornerRadius="40dp">

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/cardbg7" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewofidentify"
            android:layout_width="330dp"
            android:layout_height="120dp"
            android:layout_marginLeft="49dp"
            android:layout_marginTop="290dp"
            app:cardBackgroundColor="#9FEDF4"

            app:cardCornerRadius="40dp">

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/cardbg10" />
        </androidx.cardview.widget.CardView>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="298dp"
        android:layout_height="41dp"
        app:cardBackgroundColor="#E5F1ED"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35000002">

        <EditText
            android:id="@+id/textView17"
            android:layout_width="255dp"
            android:layout_height="match_parent"
            android:hint="Search..."
            android:paddingLeft="20pt"
            android:paddingTop="5dp"
            android:textColor="#AFADAD"

            android:textSize="20dp" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="44dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="256dp"
            android:padding="8dp"
            app:srcCompat="@drawable/search23" />

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView18"
        android:layout_width="82dp"
        android:layout_height="23dp"
        android:fontFamily="@font/audiowide"
        android:text="File"
        android:textColor="@color/darkgreen"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="169dp"
        android:layout_height="49dp"
        android:fontFamily="@font/audiowide"
        android:text="Tracker"
        android:textColor="@color/darkgreen"
        android:textSize="37dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.248" />

    <ImageView
        android:id="@+id/imageViewofsetting"
        android:layout_width="43dp"
        android:layout_height="43dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        app:srcCompat="@drawable/settings3" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="99dp"
        android:layout_height="67dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.231"
        app:srcCompat="@drawable/folder2" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView7"
        android:layout_width="409dp"
        android:layout_height="50dp"
        android:padding="0dp"
        app:cardBackgroundColor="#0D522C"
        app:cardCornerRadius="40dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/www2" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginLeft="180dp"

            android:background="#fff"
            android:backgroundTint="#0D522C"
            app:srcCompat="@drawable/home7" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginLeft="311dp"
            app:srcCompat="@drawable/instock2" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="92dp"
            android:layout_height="18dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:text="Change URL"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="92dp"
            android:layout_height="18dp"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="30dp"


            android:shadowRadius="@integer/material_motion_duration_long_1"
            android:text="Home"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="14dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="92dp"
            android:layout_height="18dp"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="30dp"
            android:text="Stock Take"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="14dp" />

    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="40dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="43dp" />


</androidx.constraintlayout.widget.ConstraintLayout>